NAME   = pmknode
CPU    = 0.1
MEMORY = 256
DISK   = [ 
	IMAGE_ID = 80
]

NIC    = [ NETWORK_ID = 2 ]

FEATURES=[ acpi="no" ]

OS     = [
  arch = x86_64
]

GRAPHICS = [
  TYPE    = "vnc",
  LISTEN  = "0.0.0.0"
]

CONTEXT=[
  HOSTNAME = "$NAME-$VMID",
  ETH0_IP = "$NIC[IP, NETWORK=\"VM-network\"]",
  ETH0_NETWORK = "$NETWORK[NETWORK_ADDRESS, NETWORK=\"VM-network\"]",
  ETH0_MASK = "$NETWORK[NETMASK, NETWORK=\"VM-network\"]",
  ETH0_GATEWAY = "$NETWORK[GATEWAY, NETWORK=\"VM-network\"]",
  ETH0_DNS = "$NETWORK[DNS, NETWORK=\"VM-network\"]",
  FILES_DS = "$FILE[IMAGE_ID=43] $FILE[IMAGE_ID=135]",
  INIT_SCRIPTS = "start_calculator",
  SSH_PUBLIC_KEY = "ssh-dss  soa08@fs2"
]
